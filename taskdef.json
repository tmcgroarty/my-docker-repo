{
  "family": "task-def-to-build-container",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::737361577962:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::737361577962:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "ubuntu-container",
      "image": "737361577962.dkr.ecr.us-east-2.amazonaws.com/myrepo:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ]
}

